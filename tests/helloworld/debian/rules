#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_clean:
	cargo clean || true

override_dh_auto_build:
	cargo build --release

override_dh_auto_test:
	# Skip tests

override_dh_auto_install:
	install -D -m 0755 target/release/helloworld debian/helloworld/usr/bin/helloworld

override_dh_dwz:
	# Skip dwz - no debug info

override_dh_strip:
	# Already stripped via Cargo profile
